@model LaundryGo.Models.Service_Provider

@{
	ViewData["Title"] = "Create";
}

<div class="row">
	<div class="col-lg-12">
		<h1 class="page-header">Service Provider</h1>
	</div>
	<!-- /.col-lg-12 -->
</div>
<div class="row">
	<div class="col-lg-12">
		<div class="panel panel-default">
			<div class="panel-heading">
				Create Service Provider
			</div>
			<div class="panel-body">
				<div class="row">
					<div class="col-md-4">
						<form asp-action="Create">
							<div asp-validation-summary="ModelOnly" class="text-danger"></div>
							<div class="form-group">
								<label asp-for="sp_name" class="control-label"></label>
								<input asp-for="sp_name" class="form-control" />
								<span asp-validation-for="sp_name" class="text-danger"></span>
							</div>
							<div class="form-group">
								<label asp-for="sp_add_details" class="control-label"></label>
								<input asp-for="sp_add_details" class="form-control" />
								<span asp-validation-for="sp_add_details" class="text-danger"></span>
							</div>
							<div class="form-group">
								<label asp-for="sp_registration_no" class="control-label"></label>
								<input asp-for="sp_registration_no" class="form-control" />
								<span asp-validation-for="sp_registration_no" class="text-danger"></span>
							</div>
							<div class="form-group">
								<label asp-for="sp_fb" class="control-label"></label>
								<input asp-for="sp_fb" class="form-control" />
								<span asp-validation-for="sp_fb" class="text-danger"></span>
							</div>
							<div class="form-group">
								<label asp-for="sp_google" class="control-label"></label>
								<input asp-for="sp_google" class="form-control" />
								<span asp-validation-for="sp_google" class="text-danger"></span>
							</div>
							<div class="form-group">
								<label asp-for="sp_phone" class="control-label"></label>
								<input asp-for="sp_phone" class="form-control" />
								<span asp-validation-for="sp_phone" class="text-danger"></span>
							</div>
							<div class="form-group">
								<label asp-for="sp_mobile" class="control-label"></label>
								<input asp-for="sp_mobile" class="form-control" />
								<span asp-validation-for="sp_mobile" class="text-danger"></span>
							</div>
							<div class="form-group">
								<label asp-for="sp_password" class="control-label"></label>
								<input asp-for="sp_password" class="form-control" />
								<span asp-validation-for="sp_password" class="text-danger"></span>
							</div>
							<div class="form-group">
								<label asp-for="sp_status" class="control-label"></label>
								<select asp-for="sp_status" asp-items="Html.GetEnumSelectList<OrderStatusType>()" class="form-control"></select>

								<span asp-validation-for="sp_status" class="text-danger"></span>
							</div>
							<div class="form-group">
								<label asp-for="sp_add_1" class="control-label"></label>
								<input asp-for="sp_add_1" class="form-control" />
								<span asp-validation-for="sp_add_1" class="text-danger"></span>
							</div>
							<div class="form-group">
								<label asp-for="sp_add_2" class="control-label"></label>
								<input asp-for="sp_add_2" class="form-control" />
								<span asp-validation-for="sp_add_2" class="text-danger"></span>
							</div>
							<div class="form-group">
								<label asp-for="sp_add_3" class="control-label"></label>
								<input asp-for="sp_add_3" class="form-control" />
								<span asp-validation-for="sp_add_3" class="text-danger"></span>
							</div>
							<div class="form-group">
								<label asp-for="sp_post" class="control-label"></label>
								<input asp-for="sp_post" class="form-control" />
								<span asp-validation-for="sp_post" class="text-danger"></span>
							</div>
							<div class="form-group">
								<input type="submit" value="Create" class="btn btn-default" />
							</div>
						</form>
					</div>
				</div>

				<div>
					<a asp-action="Index">Back to List</a>
				</div>

				</div></div></div></div>
				@section Scripts {
					@{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
				}
