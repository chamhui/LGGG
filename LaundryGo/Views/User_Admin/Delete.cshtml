@model LaundryGo.Models.User_Admin

@{
	ViewData["Title"] = "Delete";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
	<div class="col-lg-12">
		<h1 class="page-header">Admin User</h1>
	</div>
	<!-- /.col-lg-12 -->
</div>
<div class="row">
	<div class="col-lg-12">
		<div class="panel panel-default">
			<div class="panel-heading">
				Details
			</div>
			<div class="panel-body">
				<div class="row">
					<h4>Are you sure you want to delete this?</h4>
					<hr />
					<dl class="dl-horizontal">
						<dt>
							@Html.DisplayNameFor(model => model.user_name)
						</dt>
						<dd>
							@Html.DisplayFor(model => model.user_name)
						</dd>
						<dt>
							@Html.DisplayNameFor(model => model.email_address)
						</dt>
						<dd>
							@Html.DisplayFor(model => model.email_address)
						</dd>
						<dt>
							@Html.DisplayNameFor(model => model.user_password)
						</dt>
						<dd>
							@Html.DisplayFor(model => model.user_password)
						</dd>
					</dl>

					<form asp-action="Delete">
						<input type="hidden" asp-for="user_id" />
						<input type="submit" value="Delete" class="btn btn-default" /> |
						<a asp-action="Index">Back to List</a>
					</form>
				</div>
			</div>
		</div>
	</div>
</div>
