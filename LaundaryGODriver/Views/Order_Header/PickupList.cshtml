<!-- start content -->
        <!-- Header -->
        <div class="header bg-gradient-warning pb-8 pt-5 pt-md-8">
            <div class="container-fluid">
                <div class="header-body">
                
                </div>
            </div>
        </div>
        <!-- Page content -->
        <div class="container-fluid mt--7">
            <div class="row">
                <div class="col-xl-12">
                    <div class="table-responsive">
                        <table class="table align-items-center table-light" id="myTable">
                            <thead class="thead-dark">
                                <tr>
                                    <th>
                                        @Html.DisplayNameFor(model => model.cust_id)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.cust_name)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.pickup_dri_id)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.pickup_dri_name)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.sp_id)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.sp_name)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.status_id)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.order_weight)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.order_pickup_add)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.order_deliver_add)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.deliver_dri_id)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.deliver_dri_name)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.request_time)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.awaiting_pickup_time)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.pickup_time)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.washing_time)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.washing_completed_time)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.delivering_time)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.delivered_time)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.order_amount)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.payment_method)
                                    </th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.cust_id)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.cust_name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.pickup_dri_id)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.deliver_dri_name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.sp_id)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.sp_name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.status_name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.order_weight)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.order_pickup_add)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.order_deliver_add)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.deliver_dri_id)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.deliver_dri_name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.request_time)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.awaiting_pickup_time)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.pickup_time)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.washing_time)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.washing_completed_time)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.delivering_time)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.delivered_time)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.order_amount)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.payment_method)
                                    </td>
                                    <td>
                                        @*<a asp-action="ToWashing" asp-route-id="@item.order_id">Sent to Service Provider</a> |*@
                                        <a asp-action="BackAwaitPickUpList" asp-route-id="@item.order_id">Undo</a>
                                        @*<a asp-action="Details" asp-route-id="@item.order_id">Details</a> |
                                        <a asp-action="Delete" asp-route-id="@item.order_id">Delete</a>*@
                                    </td>
                                </tr>
                                }
                    
                            </tbody>
                        </table>
                    
                    </div>
                </div>
            </div>
            <div class="row mt-5">
                <div class="col-xl-8 mb-5 mb-xl-0">
                    
                </div>
                <div class="col-xl-4">
                    
                </div>
            </div>
        </div>
        <!-- end content -->

